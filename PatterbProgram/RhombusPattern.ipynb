    "Rhombus pattern"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 12,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "* * * * * \n",
      " * * * * * \n",
      "  * * * * * \n",
      "   * * * * * \n",
      "    * * * * * \n"
     ]
    }
   ],
   "source": [
    "k=0\n",
    "def Rhombus(n):\n",
    "    k=0\n",
    "    while k!=n:\n",
    "        for i in range(k):\n",
    "            print(\" \",end='')\n",
    "        for i in range(n):\n",
    "\n",
    "            print(\"* \",end='')\n",
    "        print(\"\")\n",
    "        k+=1\n",
    "        \n",
    "Rhombus(5)"
   ]
  }
