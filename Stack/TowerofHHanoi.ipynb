    "def Toh(n,Source,Aux,Dest):\n",
    "    if(n==1):\n",
    "        print(Source,\"==>\",Dest)\n",
    "    else:\n",
    "        Toh(n-1,Source,Dest,Aux)\n",
    "        print(Source,\"==>\",Dest)\n",
    "        Toh(n-1,Aux,Source,Dest)\n",
    "\n"
   ]
